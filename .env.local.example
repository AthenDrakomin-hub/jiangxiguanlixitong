# Upstash Redis Configuration
# Get these values from your Upstash Redis database settings

KV_REST_API_URL=your_upstash_redis_rest_api_url_here
KV_REST_API_TOKEN=your_upstash_redis_rest_api_token_here

# Optional: Read-only token for additional security
KV_REST_API_READ_ONLY_TOKEN=your_upstash_redis_read_only_token_here

# Alternative variable names (if the above don't work)
UPSTASH_REDIS_URL=your_upstash_redis_url_here
UPSTASH_REDIS_TOKEN=your_upstash_redis_token_here

# Hotel-specific variable names (highest priority)
HOTEL_KV_REST_API_URL=your_hotel_kv_rest_api_url_here
HOTEL_KV_REST_API_TOKEN=your_hotel_kv_rest_api_token_here

# Administrator account credentials (REQUIRED for login)
VITE_ADMIN_USER=your_admin_username_here
VITE_ADMIN_PASS=your_strong_admin_password_here